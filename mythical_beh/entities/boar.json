      "time_schedule": {
        "minecraft:scheduler": {
          "scheduled_events": [
            {
              "scheduled_event": {
                "filters": {
                  "all_of": [
                    { "test": "is_daytime", "operator": "!=", "value": true }
                  ]
                },
                "event": "boar_configure_night"
              }
            },
            {
              "scheduled_event": {
                "filters":{
                  "all_of": [
                    { "test": "is_daytime", "operator": "==", "value": true }
                  ]
                },
                "event": "boar_configure_day"
              }
            }
          ]
        }
      },
      "boar_night": {
